{
	"name": "hugofy",
	"publisher": "ttys3",
	"repository": "https://github.com/ttys3/hugofy-vscode",
	"displayName": "hugofy",
	"description": "modified hugofy extension with extra support for hugo page bundles",
	"version": "0.2.6",
	"icon": "hugo.png",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"hugo",
		"hugofy",
		"ttys3",
		"page bundles"
	],
	"recommendations": [
		"telesoho.vscode-markdown-paste-image",
		"budparr.language-hugo-vscode",
		"kaellarkin.hugo-shortcode-syntax",
		"fivethree.vscode-hugo-snippets",
		"yzhang.markdown-all-in-one",
		"bungcip.better-toml",
		"Iceyer.toml-formatter",
		"shuworks.vscode-table-formatter",
		"DavidAnson.vscode-markdownlint",
		"mdickin.markdown-shortcuts",
		"naumovs.color-highlight",
		"eliostruyf.vscode-front-matter"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"menus": {
			"explorer/context": [
				{
					"when": "explorerResourceIsFolder",
					"command": "hugofy.newPost",
					"group": "hugo"
				}
			]
		},
		"commands": [
			{
				"command": "hugofy.build",
				"title": "Hugofy: Build"
			},
			{
				"command": "hugofy.getVersion",
				"title": "Hugofy: Version"
			},
			{
				"command": "hugofy.downloadTheme",
				"title": "Hugofy: Download Theme"
			},
			{
				"command": "hugofy.newPost",
				"title": "Hugofy: New Post"
			},
			{
				"command": "hugofy.newSite",
				"title": "Hugofy: New Site"
			},
			{
				"command": "hugofy.setTheme",
				"title": "Hugofy: Set Theme"
			},
			{
				"command": "hugofy.startServer",
				"title": "Hugofy: Start Server"
			},
			{
				"command": "hugofy.stopServer",
				"title": "Hugofy: Stop Server"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack --mode none",
		"lint": "eslint src --ext ts",
		"watch": "webpack --mode none --watch",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.44.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"ts-loader": "^7.0.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11"
	},
	"dependencies": {
		"@types/follow-redirects": "^1.8.0",
		"follow-redirects": "^1.11.0",
		"open": "^7.0.3",
		"transliteration": "^2.1.8",
		"vscode-cache": "^0.3.0"
	}
}
