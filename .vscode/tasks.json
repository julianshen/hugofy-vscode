// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
// `$ts-webpack` need: `ext install eamodio.tsl-problem-matcher`
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "watch",
			"problemMatcher": "$ts-webpack",
			"isBackground": true,
			"presentation": {
				"reveal": "never"
			},
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}
